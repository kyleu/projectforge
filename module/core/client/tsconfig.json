{
  "compilerOptions": {
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "alwaysStrict": true,
    "baseUrl": "./src",
    "declaration": true,
    "esModuleInterop": true,{{{ if .HasModule "jsx" }}}
    "jsx": "react",
    "jsxFactory": "JSX",{{{ end }}}
    "listFiles": false,
    "module": "none",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outFile": "../assets/client.js",{{{ .TypeScriptPaths }}}
    "removeComments": true,
    "preserveConstEnums": true,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES6"
  },
  "include": ["**/*"],
  "exclude": ["node_modules"]
}
