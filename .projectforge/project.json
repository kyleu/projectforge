{
  "key": "projectforge",
  "name": "Project Forge",
  "icon": "projectforge",
  "version": "2.0.4",
  "package": "projectforge.dev/projectforge",
  "port": 40000,
  "modules": [
    "core",
    "android",
    "demo",
    "desktop",
    "docbrowse",
    "export",
    "filesystem",
    "git",
    "help",
    "ios",
    "jsonschema",
    "jsx",
    "marketing",
    "mcp",
    "metamodel",
    "metaschema",
    "notarize",
    "playwright",
    "plot",
    "process",
    "richedit",
    "search",
    "task",
    "themecatalog",
    "types",
    "uidocs",
    "upgrade",
    "wasmserver",
    "websocket"
  ],
  "ignore": [
    "^data",
    "^module",
    "^testproject",
    "^assets/module"
  ],
  "tags": [
    "kyleu",
    "self"
  ],
  "info": {
    "org": "kyleu",
    "authorID": "kyleu",
    "authorName": "Kyle U",
    "authorEmail": "projectforge@kyleu.com",
    "license": "MIT",
    "homepage": "https://projectforge.dev",
    "sourcecode": "https://github.com/kyleu/projectforge",
    "summary": "Build and maintain feature-rich applications using Golang",
    "description": "Project Forge creates and manages applications written in the Go programming language. Using code generation, the latest technology, and a strong standard library, Project Forge will help manage your project's code, assets, UI and UX.",
    "bundle": "com.kyleu.projectforge",
    "signingIdentity": "${APPLE_SIGNING_IDENTITY}",
    "javaPackage": "dev.projectforge",
    "ignoredFiles": [
      "app/site/handle.go",
      "app/site/menu.go",
      "views/vsite/Index.html"
    ]
  },
  "build": {
    "changelog": true,
    "desktop": true,
    "notarize": true,
    "signing": true,
    "android": true,
    "iOS": true,
    "wasm": true,
    "x86": true,
    "windowsARM": true,
    "linuxARM": true,
    "linuxMIPS": true,
    "linuxOdd": true,
    "aix": true,
    "dragonfly": true,
    "illumos": true,
    "freeBSD": true,
    "netBSD": true,
    "openBSD": true,
    "solaris": true,
    "homebrew": true,
    "nfpms": true,
    "snapcraft": true
  }
}
